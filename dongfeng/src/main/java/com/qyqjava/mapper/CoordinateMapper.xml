	<update id="updateByPrimaryKeySelective"
		parameterType="com.qyqjava.pojo.Coordinate">
		update coordinate
		<set>
			<if test="nameid != null">
				nameId = #{nameid,jdbcType=VARCHAR},
			</if>
			<if test="x != null">
