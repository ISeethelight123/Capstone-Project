			</if>
			<if test="y != null">
				y = #{y,jdbcType=DOUBLE},
			</if>
		</set>
		where no = #{no,jdbcType=INTEGER}
	</update>
	<update id="updateByPrimaryKey"
		parameterType="com.qyqjava.pojo.Coordinate">
		update coordinate
		set nameId = #{nameid,jdbcType=VARCHAR},
		x = #{x,jdbcType=DOUBLE},
		y = #{y,jdbcType=DOUBLE}
		where no = #{no,jdbcType=INTEGER}
	</update>

	<!-- 查找仓库的坐标 -->
	<select id="showXY" resultType="Map">
		SELECT coor.x,coor.y FROM wrehouse wre,coordinate coor WHERE
