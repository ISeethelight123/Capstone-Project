<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lty.mapper.User_rolesMapper">

<delete id="deletJSS" parameterType="Integer">
		delete from `user_roles`
		where id=#{id}
	</delete>
	
	<insert id="addJSS" parameterType="String">
		insert into `user_roles`(username)values(#{username})
	</insert>
</mapper>