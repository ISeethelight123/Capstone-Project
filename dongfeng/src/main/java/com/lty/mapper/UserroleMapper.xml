<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lty.mapper.UserroleMapper">

<insert id="addpowerNumber" parameterType="com.lty.entity.Userrole">
		insert into
		userrole(uid,rid)values(#{uid},#{rid})
	</insert>
<delete id="deletePower" parameterType="Integer">
		delete from userrole
		where uid=#{uid}
	</delete>
</mapper>